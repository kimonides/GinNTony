   def char[] reverse (char[] s):
      int i, l
      char[] result
      l := length(s)
      result := new char[l+1]
      for i := 0; i < l; i := i+1:
         result[i] := s[l-i-1]
      end
      result[i] := '\0'
      return result
   end

