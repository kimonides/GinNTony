def number ():
   int i, j, k
   for i := 1; i < 10; i := i+1:
      for j := 0; j < 10; j := j+1:
         for k := 0; k < 10; k := k+1:
            if 100*i + 10*j + k = i*i*i +j*j*j +k*k*k:
               puti(100*i+10*j+k)
               puts("\n")
            end
         end
      end
   end
end
